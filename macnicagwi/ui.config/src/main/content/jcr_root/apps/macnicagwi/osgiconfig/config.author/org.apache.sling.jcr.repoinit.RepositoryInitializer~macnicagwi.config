        allow jcr:read,rep:write,jcr:nodeTypeManagement,jcr:modifyProperties,jcr:lockManagement,jcr:versionManagement,jcr:addChildNodes,jcr:nodeTypeManagement on /content/macnicagwi
        allow jcr:read,rep:write,jcr:nodeTypeManagement,jcr:modifyProperties,jcr:lockManagement,jcr:versionManagement,jcr:addChildNodes,jcr:nodeTypeManagement on /content/experience-fragments/macnicagwi
