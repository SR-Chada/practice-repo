        allow jcr:read,rep:write,jcr:nodeTypeManagement,jcr:modifyProperties,jcr:lockManagement,jcr:versionManagement,jcr:addChildNodes,jcr:nodeTypeManagement on /content/dam/macnicagwi
        allow jcr:read,jcr:nodeTypeManagement,jcr:modifyProperties,jcr:lockManagement,jcr:versionManagement,jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement on /conf

    end

set ACL for content-authors
