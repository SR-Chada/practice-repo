create service user macnicagwi-wcm-system-service  with forced path system/cq:services/macnicagwi
    add macnicagwi-wcm-system-service to group administrators
    set principal ACL for macnicagwi-wcm-system-service
        allow jcr:all,crx:replicate,rep:write on /home/users
        allow jcr:all,crx:replicate,rep:write on /home/groups
        allow jcr:all on /var/workflow/packages
		allow jcr:all,crx:replicate,rep:write on /content/macnicagwi
		allow jcr:all,crx:replicate,rep:write on /content/dam/macnicagwi
		allow jcr:all,crx:replicate,rep:write on /content/cq:tags
		allow jcr:all,crx:replicate,rep:write on /conf
		allow jcr:all,crx:replicate,rep:write on /conf/macnicagwi
		allow jcr:all,crx:replicate,rep:write on /conf/globalportal
        allow jcr:all on /conf/global/settings/workflow/models
        allow jcr:all on /var/workflow
        allow jcr:all on /var/workflow/instances
        allow jcr:all on /var/statistics/workflows
        allow jcr:all on /libs/settings/workflow/models
        allow jcr:all on /var/cq/dtm-reactor/clientlibs
        allow jcr:all on /etc/workflow
        allow jcr:all on /content/experience-fragments/macnicagwi/apac
        allow jcr:all on /content/experience-fragments/macnicagwi
        allow jcr:all on /content/experience-fragments/macnicagwi/globalportal
end

